<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root fx:id="box_moviestars" alignment="CENTER_LEFT" stylesheets="@../style.css" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<HBox fx:id="box_moviestars_score" alignment="CENTER_LEFT" prefHeight="32.0">
			<children>
				<Label fx:id="movie_score" text="Label">
					<HBox.margin>
						<Insets left="8.0" />
					</HBox.margin>
				</Label>
				<ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../images/icon_star_16_16.png" />
					</image>
					<HBox.margin>
						<Insets left="4.0" right="4.0" />
					</HBox.margin>
				</ImageView>
			</children>
		</HBox>
		<Label fx:id="movie_name" text="Label">
			<HBox.margin>
				<Insets left="8.0" right="8.0" />
			</HBox.margin>
		</Label>
	</children>
</fx:root>
